@model IQueryable<StudentFarm.Models.Crop>

@{
    ViewBag.Title = "Welcome";
}

@section subhead {
<h2>Welcome</h2>
}

<table>
    <tr>
        <th>Name</th>
        <th>Added Date</th>
        <th>Description</th>
        <th>Organic?</th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>@item.Name</td>
        <td>@item.AddedDate</td>
        <td>@item.Description</td>
        <td>
            @if (@item.Organic) {
                @:Yes
            }
            else {
                @:No
            }
        </td>
    </tr>
}
</table>